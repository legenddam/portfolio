<h1>Add Proejct</h1>
<form method = 'post' action='/admin/add', enctype="multipart/form-data">
    <div class='form-gruop'>
        <label>Title</label>
        <input type="text", class="form-control", name="title" placeholder="Title">
    </div>
    <div class='form-group'>
        <label>Service</label>
        <input type="text", class="form-control", name="service" placeholder="Service">
    </div>
    <div class='form-gruop'>
        <label>Client</label>
        <input type="text", class="form-control", name="client" placeholder="Client">
    </div>

    <div class='form-group'>
        <label>Description</label>
        <textarea class="form-control", name="description"></textarea>

    </div>

    <div class='form-gruop'>
        <label>ProjectURL</label>
        <input type="text", class="form-control", name="projecturl" placeholder="ProjectURL">
    </div>
    
    <div class='form-gruop'>
        <label>ProjectDate</label>
        <input type="date", class="form-control", name="projectdate">
    </div>
    <div class='form-group'>
        <label>Image</label>
        <input type="file", class="form-control", name="projectimage">
    </div>
    <input type="submit" class="form-control" value="Add">
</form>